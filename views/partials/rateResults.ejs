<table class="table">
  <% if (results.length) { %>
    <tr>
      <td>Carrier</td>
      <td>Service</td>
      <td>Rate</td>
      <td>Online Rate</td>
      <td>Expected Delivery Date</td>
    </tr>
    <% for (var i = 0; i < results.length; i++) { %>
      <tr>
        <td><%= results[i].carrier %></td>
        <td><%= results[i].serviceName %></td>
        <td><%= results[i].regularRate %></td>
        <td><%= results[i].onlineRate %></td>
        <td><%= results[i].deliveryDate %></td>
      </tr>
    <% } %>
  <% } else { %>
    <p>No services available for your request.</p>
  <% } %>
</table>
